        "# https://bibtexparser.readthedocs.io/en/master/tutorial.html#step-2-parse-it\n",
        "file_name = list(uploaded.keys())[0] \n",
        "with open(file_name) as bibtex_file:\n",
        "    bib_database = bibtexparser.load(bibtex_file)\n",
        "        \n",
        "df = pd.DataFrame(bib_database.entries) \n",
        "# print(bib_database.entries) \n",
        "\n",
        "# bib_database.entries -- everything \n",
        "\n",
        "# selection = df[['doi', 'number']]\n",
        "# selection.to_csv('temp.csv', index=False)   \n",
        "\n",
        "df.to_csv('references.csv', index=False)   \n",
        "\n",
        "# df.columns\n",
        "# Index(['abstract', 'publisher', 'year', 'author', 'title', 'ENTRYTYPE', 'ID',\n",
        "#        'pages', 'number', 'volume', 'journal'],\n",
        "#       dtype='object')\n",
        "\n",
        "df_print = df.sort_values('year',ignore_index=True) #['year']\n",
        "\n",
        "print('Topic: On women’s movements in the Middle East and North Africa \\n\\n')\n",
        "for i in range(len(df_print['year'])):\n",
        "    try: \n",
        "        print('%i.'%(i+1))\n",
        "        print('Title: ', df_print['title'][i] )\n",
        "        print('Authors: ', df_print['author'][i] )\n",
        "        print('Year: ', df_print['year'][i] )\n",
        "        print('Journal: ', df_print['journal'][i] ) \n",
        "        print('Publisher: ', df_print['publisher'][i])\n",
        "        print('Description:' if df_print['ENTRYTYPE'][i]=='book' else 'Abstract:', df_print['abstract'][i],'\\n\\n')  \n",
        "    except(KeyError):\n",
        "        continue \n"
